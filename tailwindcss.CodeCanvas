		},
		"c373f9ca-351d-4a67-af50-2570e9e52d92": {
			"path": "c373f9ca-351d-4a67-af50-2570e9e52d92",
			"cellName": "Scanner Initialization (Rust/N-API) - lib.rs:L111-119",
